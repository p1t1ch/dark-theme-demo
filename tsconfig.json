{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@theme": ["config/theme.ts"],
      "@/*": ["src/*"]
    },
    "types": ["node", "@emotion/core"]
  }
}
